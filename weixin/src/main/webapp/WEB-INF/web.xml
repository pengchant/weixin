<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<listener>
		<listener-class>com.handkoo.util.ContextListener</listener-class>
	</listener> 
	<servlet>
		<servlet-name>CoreServlet</servlet-name>
		<display-name>CoreServlet</display-name>
		<description></description>
		<servlet-class>com.handkoo.servlet.CoreServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>QRCodeServlet</servlet-name>
		<display-name>QRCodeServlet</display-name>
		<description></description>
		<servlet-class>com.handkoo.servlet.QRCodeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>JSSDKConfigServlet</servlet-name>
		<display-name>JSSDKConfigServlet</display-name>
		<description></description>
		<servlet-class>com.handkoo.servlet.JSSDKConfigServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CoreServlet</servlet-name>
		<url-pattern>/CoreServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>QRCodeServlet</servlet-name>
		<url-pattern>/QRCodeServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>JSSDKConfigServlet</servlet-name>
		<url-pattern>/JSSDKConfigServlet</url-pattern>
	</servlet-mapping> 
</web-app>
